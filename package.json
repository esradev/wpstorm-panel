{
  "name": "wpstorm-panel",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:admin": "npm-run-all --sequential buildwp:admin",
    "start:admin": "npm-run-all --parallel wpstart:admin",
    "build:panel": "npm-run-all --sequential buildwp:panel",
    "start:panel": "npm-run-all --parallel wpstart:panel",
    "buildwp:admin": "wp-scripts build --webpack-src-dir=src/admin/ --output-path=build/admin/",
    "wpstart:admin": "wp-scripts start --webpack-src-dir=src/admin/ --output-path=build/admin/",
    "buildwp:panel": "wp-scripts build --webpack-src-dir=src/panel/ --output-path=build/panel/",
    "wpstart:panel": "wp-scripts start --webpack-src-dir=src/panel/ --output-path=build/panel/",
    "check-engines": "wp-scripts check-engines",
    "check-licenses": "wp-scripts check-licenses",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "lint:md:docs": "wp-scripts lint-md-docs",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "packages-update": "wp-scripts packages-update",
    "plugin-zip": "wp-scripts plugin-zip",
    "test:e2e": "wp-scripts test-e2e",
    "test:unit": "wp-scripts test-unit-js"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@wordpress/element": "^6.15.1",
    "@wordpress/i18n": "^5.15.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.469.0",
    "npm-run-all": "^4.1.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@wordpress/scripts": "^27.9.0",
    "autoprefixer": "^10.4.20",
    "browser-sync-webpack-plugin": "^2.3.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17"
  }
}
